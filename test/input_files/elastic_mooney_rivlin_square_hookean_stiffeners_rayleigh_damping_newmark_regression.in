##### Mesh related options #####
[Mesh]
   [./Read]
      filename = './grids/mixed_dim_symmetric_square.exo'
[]

[Materials]
   [./Membrane]
      [./Density]
        value = '1.0'
      [../MembraneThickness]
        value = '0.05'
      [../StressStrainLaw]
        model = 'incompressible_hyperelasticity'
        strain_energy = 'mooney_rivlin'
        [./MooneyRivlin]
           C1 = '24'
           C2 = '3.0'
   [../../../Cable]
      [./Density]
         value = '1.0'
      [../CrossSectionalArea]
         value = '0.1'
      [../StressStrainLaw]
         model = 'hookes_law'
         [./HookesLaw]
            E = '10000'
            nu = '0.3'
[]

[Physics]
   enabled_physics = 'ElasticMembrane
                      ElasticMembraneConstantPressure
                      ElasticCable
                      ElasticMembraneRayleighDamping
                      ElasticCableRayleighDamping'

   [./ElasticMembrane]

      material = 'Membrane'
      enabled_subdomains = '1'

      bc_ids   = '1 2 3'
      bc_types = 'pinned yz_symmetry xz_symmetry'

   [../ElasticMembraneConstantPressure]

      pressure = '1.0'

   [../ElasticMembraneRayleighDamping]
     
      lambda_factor = '0.1'
      mu_factor = '0.1'

   [../ElasticCable]

      material = 'Cable'
      enabled_subdomains = '2'

   [../ElasticCableRayleighDamping]

      lambda_factor = '0.1'
      mu_factor = '0.1'
[]

[Variables]
   [./Displacement]
      names = 'u v w'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]

[SolverOptions]
   [./TimeStepping]
      solver_type = 'libmesh_newmark'
      delta_t = '0.025'
      n_timesteps = '10'
[]

[linear-nonlinear-solver]
relative_residual_tolerance = '1.0e-11'
relative_step_tolerance = '1.0e-11'

max_nonlinear_iterations =  '50'
max_linear_iterations = '2500'

verify_analytic_jacobians = '0.0e-6'

initial_linear_tolerance = '1.0e-10'

use_numerical_jacobians_only = 'false'

[]

##### Visualization options ######
[vis-options]
output_vis = 'false'

vis_output_file_prefix = 'sheet'

output_residual = 'false'

output_format = 'ExodusII xdr'

##### Options for print info to the screen #####
[screen-options]

system_name = 'StretchedElasticSheet'

print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

print_element_jacobians = 'false'
print_element_residuals = 'false'

[]
