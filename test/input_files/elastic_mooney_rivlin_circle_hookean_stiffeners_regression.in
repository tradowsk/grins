# Materials options
[Materials]
  [./Membrane]
    [./MembraneThickness]
      value = '0.05'
    [../StressStrainLaw]
      model = 'incompressible_hyperelasticity'
      strain_energy = 'mooney_rivlin'
      [./MooneyRivlin]
         C1 = '24'
         C2 = '3.0'
    [../../Density]
       value = '1.0'

  [../../Cable]
    [./Density]
      value = '1.0'
    [../CrossSectionalArea]
      value = '0.1'
    [../StressStrainLaw]
      model = 'hookes_law'
      [./HookesLaw]
         nu = '0.3'
         E= '10000'
[]

##### Mesh related options #####
[Mesh]
   [./Read]
      filename = './grids/mixed_dim_circle.exo'
[]

##### Options related to all Physics #####
[Physics]
enabled_physics = 'ElasticMembrane:Membrane1
                   ElasticMembraneConstantPressure:Membrane1
                   ElasticCable:Cables'

##### Options for Elastic Membrane Physics #####
[./ElasticMembrane:Membrane1]

material = 'Membrane'

enabled_subdomains = '1'

bc_ids   = '1'
bc_types = 'pinned'

ic_ids       = '1'
ic_types     = 'parsed'
ic_variables = 'w'
ic_values    = '-0.5*cos(pi/2*sqrt(x^2+y^2))'

[../ElasticMembraneConstantPressure:Membrane1]

pressure = '1.0'

##### Options for Elastic Cable Physics #####
[../ElasticCable:Cables]

material = 'Cable'

enabled_subdomains = '2'

[]

[Variables]
   [./Displacement]
      names = 'u v w'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]

##### Linear and nonlinear solver options #####
[linear-nonlinear-solver]
relative_residual_tolerance = '1.0e-11'
relative_step_tolerance = '1.0e-11'

max_nonlinear_iterations =  '50'
max_linear_iterations = '2500'

verify_analytic_jacobians = '0.0'

initial_linear_tolerance = '1.0e-10'

use_numerical_jacobians_only = 'false'

[]

##### Visualization options ######
[vis-options]
output_vis = 'false'

vis_output_file_prefix = 'sheet'

output_residual = 'false'

output_format = 'ExodusII xdr'

##### Options for print info to the screen #####
[screen-options]

system_name = 'StretchedElasticSheet'

print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

print_element_jacobians = 'false'
print_element_residuals = 'false'

[]
